<div id="enter-food-form">
  <%= form_for [@user, @food], remote: true do |f| %>
    <div class="field">
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :calories %>
      <%= f.text_field :calories %>
      <%= f.submit %>
    </div>
  <% end %>
</div>

<button onclick='hideNewForm()' id="food-cancel-button">Cancel</button>
